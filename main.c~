#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(int argc, char **argv)
{
	if (argc > 1)
	{
		char arr[] = "testdememset";
		int c;
		int len;
		int i;
		
		i = 0;
		c = atoi(argv[1]);
		len = 20;
		memset(arr, c, len);
//		while (arr[i] != "\0")
			printf("%s", arr);
	}
	return (0);

}
